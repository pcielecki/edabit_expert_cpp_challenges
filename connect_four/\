#include <gtest/gtest.h>
#include "connect_four.hpp"

TEST(ConnectFourSuite, ConnectFourTest) {
    vector<string> moves {
        "A_Red",
        "B_Yellow",
        "A_Red",
        "B_Yellow",
        "A_Red",
        "B_Yellow",
        "G_Red",
        "B_Yellow"};
    EXPECT_EQ("Yellow", connect_four(moves));
}
